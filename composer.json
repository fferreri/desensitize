{
    "name": "fferreri/desensitize",
    "description": "Make your Laravel routes case-insensitive.",
    "keywords": ["laravel", "routing", "case-insensitive"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Federico Ferreri",
            "email": "federico@ferreri.com.ar"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0 || ^11.0",
        "illuminate/routing": "^10.0 || ^11.0",
        "illuminate/support": "^10.0 || ^11.0"
    },
    "autoload": {
        "psr-4": {
            "Waryor\\Desensitize\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Waryor\\Desensitize\\DesensitizeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
